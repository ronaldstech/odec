{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://unimarket-mw.com/codec/api/:path*"
        },
        {
            "source": "/uploads/:path*",
            "destination": "https://unimarket-mw.com/codec/uploads/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}